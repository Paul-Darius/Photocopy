<p id="notice"><%= notice %></p>

<h1>List of users</h1>
<%= link_to "User Statistics", "/listofusers/statisticsuser" %>

<table>
  <thead>
  <tr>
    <th>ID</th>
    <th>E-mail</th>
    <th>Last sign in time</th>
    <th>Count sign in</th>
    <th>Current Sign in time</th>
    <th colspan="3"></th>
  </tr>
  </thead>
  <tbody>
  <% @users.each do |user| %>
      <tr>
        <td><%= user.id %></td>
        <td><%= user.email %></td>
        <td><%= user.last_sign_in_at %></td>
        <td><%= user.sign_in_count %></td>
        <td><%= user.current_sign_in_at %></td>
      </tr>
  <% end %>
  </tbody>
</table>
<br>
